{
  "schemaVersion": 1,
  "id": "dungeonz",
  "version": "${version}",
  "name": "DungeonZ",
  "description": "This mod adds the basement for creating and exploring dungeons",
  "authors": [
    "Globox_Z",
    "Zura"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/dungeonz",
    "sources": "https://github.com/Globox1997/Dungeonz",
    "issues": "https://github.com/Globox1997/Dungeonz/issues"
  },
  "license": "MIT",
  "icon": "assets/dungeonz/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.dungeonz.DungeonzMain"
    ],
    "client": [
      "net.dungeonz.DungeonzClient"
    ],
    "modmenu": [
      "net.dungeonz.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "dungeonz.mixins.json"
  ],
  "depends": {
    "java": ">=17",
    "minecraft": "${minecraft_version}",
    "fabric-api": ">=${fabric_version}",
    "fabricloader": ">=${loader_version}",
    "cloth-config2": ">=${cloth_config_version}"
  },
  "suggests": {
    "another-mod": "*"
  },
  "custom": {
    "mc-publish": {
      "modrinth": "11jwniuC",
      "curseforge": 864150
    }
  }
}